<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Style x:Key="btn_prin" TargetType="{x:Type Button}">
        <Setter Property="Width" Value="110"/>
        <Setter Property="Height" Value="25"/>
        <Setter Property="Background" Value="#ff0f9b"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Foreground" Value="White"/>
    </Style>
    <Style x:Key="btn_reset" TargetType="{x:Type Button}">
        <Setter Property="Background" Value="#86e3e5"/>
        <Setter Property="BorderBrush" Value="#86e3e5"/>
        <Setter Property="Foreground" Value="White"/>
    </Style>
</ResourceDictionary>